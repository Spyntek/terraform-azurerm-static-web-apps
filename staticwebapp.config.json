{
    "trailingSlash": "auto",
        "routes": [
              {"route": "/YYZ/yyzpacfile.pac", "statusCode": 200,"allowedRoles": ["authenticated"]},
              
              {"route": "/YKF/ykfpacfile.pac", "statusCode": 200, "allowedRoles": ["authenticated"]},
              
              {"route": "/YUL/yulpacfile.pac", "statusCode": 200, "allowedRoles": ["authenticated"]},
              
              {"route": "/YYC/yycpacfile.pac", "statusCode": 200, "allowedRoles": ["authenticated"]},
              
              {"route": "/LAX/laxpacfile.pac", "statusCode": 200, "allowedRoles": ["authenticated"]},
              
              {"route": "/SFO/sfopacfile.pac", "statusCode": 200, "allowedRoles": ["authenticated"]},
              
              {"route": "/BHO/bhopacfile.pac", "statusCode": 200, "allowedRoles": ["authenticated"]},
              
              {"route": "/MAA/maapacfile.pac", "statusCode": 200, "allowedRoles": ["authenticated"]},
              
              {"route": "/JFK/jfkpacfile.pac", "statusCode": 200, "allowedRoles": ["authenticated"]},
              
              {"route": "/EWR/ewrpacfile.pac", "statusCode" :200, "allowedRoles": ["authenticated"]}
            ],          
    
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "globalHeaders": {
        "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'; frame-ancestors 'none'; base-uri 'none';"
    },
    "mimeTypes": {
        ".pac": "application/x-ns-proxy-autoconfig"
    },
    "responseOverrides": {
        "401": {
            "rewrite": "/login",
            "statusCode": 302
        }
    }
  }