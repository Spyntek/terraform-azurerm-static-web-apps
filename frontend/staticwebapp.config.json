{
    "trailingSlash": "auto",
        "routes": [
          {
            "route": "/pacfiles/YYC/pacfile.pac",
            "allowedRoles": ["anonymous"]
          },
          {
            "route": "/pacfiles/YYZ/pacfile.pac",
            "allowedRoles": ["anonymous"]
          },
          {
            "route": "/pacfiles/BHO/pacfile.pac",
            "allowedRoles": ["anonymous"]
          },
          {
            "route": "/pacfiles/YUL/pacfile.pac",
            "allowedRoles": ["anonymous"]
          },
          {
            "route": "/pacfiles/LAX/pacfile.pac",
            "allowedRoles": ["anonymous"]
          },
          {
            "route": "/pacfiles/JFK/pacfile.pac",
            "allowedRoles": ["anonymous"]
          },
          {
            "route": "/pacfiles/EWR/pacfile.pac",
            "allowedRoles": ["anonymous"]
          },
          {
            "route": "/pacfiles/MAA/pacfile.pac",
            "allowedRoles": ["anonymous"]
          },
          {
            "route": "/pacfiles/SFO/pacfile.pac",
            "allowedRoles": ["anonymous"]
          },
          {
            "route": "/pacfiles/YKF/pacfile.pac",
            "allowedRoles": ["anonymous"]
          }          
        ],
        "mimeTypes": {
          ".pac": "application/x-ns-proxy-autoconfig"
        },
        "navigationFallback": {
          "rewrite": "/index.html",
          "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
      },
      "globalHeaders": {
          "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'; frame-ancestors 'none'; base-uri 'none';"
      },     
      "responseOverrides": {
          "401": {
              "rewrite": "/login",
              "statusCode": 302
          }
      }
  }